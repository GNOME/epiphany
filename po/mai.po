msgid "Toolbar style"
msgstr "अओजारपट्टी शैली"

#: ../data/epiphany.schemas.in.h:52
msgid ""
"Toolbar style. Allowed values are \"\" (use GNOME default style), \"both"
"\" (text and icons), \"both-horiz\" (text besides icons), \"icons\", and "
"\"text\"."
msgstr ""
