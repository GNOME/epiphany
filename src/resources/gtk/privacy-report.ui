<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="EphyPrivacyReport" parent="AdwDialog">
    <property name="content-height">600</property>
    <property name="content-width">360</property>
    <child>
      <object class="AdwNavigationView" id="navigation_view">
        <child>
          <object class="AdwNavigationPage">
            <property name="title" translatable="yes">Overview</property>
            <property name="tag">overview</property>
            <child>
              <object class="AdwToolbarView">
                <child type="top">
                  <object class="AdwHeaderBar">
                    <property name="show-title">False</property>
                  </object>
                </child>
                <property name="content">
                  <object class="AdwStatusPage" id="status_page">
                    <property name="title" translatable="yes">Privacy Report</property>
                    <property name="icon-name">system-lock-screen-symbolic</property>
                    <style>
                      <class name="compact"/>
                    </style>
                    <child>
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="spacing">24</property>
                        <child>
                          <object class="GtkStackSwitcher">
                            <property name="stack">stack</property>
                            <property name="halign">center</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkStack" id="stack">
                            <child>
                              <object class="GtkStackPage">
                                <property name="name">website</property>
                                <property name="title" translatable="yes">Websites</property>
                                <property name="child">
                                  <object class="GtkListBox" id="website_listbox">
                                    <signal name="row-selected" handler="on_website_listbox_selected"/>
                                    <style>
                                      <class name="content"/>
                                    </style>
                                  </object>
                                </property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkStackPage">
                                <property name="name">tracker</property>
                                <property name="title" translatable="yes">Trackers</property>
                                <property name="child">
                                  <object class="GtkListBox" id="tracker_listbox">
                                    <signal name="row-selected" handler="on_tracker_listbox_selected"/>
                                    <style>
                                      <class name="content"/>
                                    </style>
                                  </object>
                                </property>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwNavigationPage" id="details_page">
            <property name="title" translatable="yes">Details</property>
            <property name="tag">details</property>
            <property name="child">
              <object class="AdwToolbarView">
                <child type="top">
                  <object class="AdwHeaderBar"/>
                </child>
                <property name="content">
                  <object class="AdwPreferencesPage" id="details_prefs_page">
                    <property name="description-centered">True</property>
                    <child>
                      <object class="AdwPreferencesGroup">
                        <child>
                          <object class="GtkListBox" id="details_listbox">
                            <property name="selection-mode">none</property>
                            <style>
                              <class name="content"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
